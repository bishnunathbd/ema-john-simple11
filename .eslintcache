ingKey];\n      return product;\n    })\n    setCart(previousCart);\n  }, [])\n\n  const handleAddProduct = (product) => {\n    const sameProduct = cart.find(pd => pd.key === product.key);\n    let count = 1;\n    let newCart;\n    if (sameProduct) {\n      count = sameProduct.quantity + 1;\n      sameProduct.quantity = count;\n      const others = cart.filter(pd => pd.key !== sameProduct.key);\n      newCart = [...others, sameProduct];\n    }\n    else {\n      product.quantity = 1;\n      newCart = [...cart, product];\n    }\n    setCart(newCart);\n    addToDatabaseCart(product.key, count);\n  }\n\n  return (\n    <div className='shop-container'>\n      <div className='product-container'>\n        {\n          products.map(product => <Product showAddToCart={true} product={product} handleAddProduct={handleAddProduct} key={product.key}></Product>)\n        }\n      </div>\n      <div className='cart-container'>\n        <Cart cart={cart}>\n          <Link to='/review'>\n            <button className='shop-btn'>Review Order</button>\n          </Link>\n        </Cart>\n      </div>\n    </div>\n  );\n};\n\nexport default Shop;","/home/bishnu/Desktop/Programming-Hero/projects/ema-john-simple/src/fakeData/index.js",[],"/home/bishnu/Desktop/Programming-Hero/projects/ema-john-simple/src/fakeData/android.js",[],["104","105"],"/home/bishnu/Desktop/Programming-Hero/projects/ema-john-simple/src/fakeData/laptop.js",[],"/home/bishnu/Desktop/Programming-Hero/projects/ema-john-simple/src/fakeData/camera.js",[],"/home/bishnu/Desktop/Programming-Hero/projects/ema-john-simple/src/components/Product/Product.js",[],"/home/bishnu/Desktop/Programming-Hero/projects/ema-john-simple/src/components/Cart/Cart.js",["106"],"import React from 'react';\nimport { Link } from 'react-router-dom';\n\nconst Cart = (props) => {\n  const cart = props.cart;\n\n  // const total = cart.reduce((total, product) => total + product.price, 0);\n  let total = 0;\n  for (let i = 0; i < cart.length; i++) {\n    const product = cart[i];\n    total = total + product.price * product.quantity;\n  }\n\n  let shipping = 0;\n  if (total > 35) {\n    shipping = 0;\n  }\n  else if (total > 15) {\n    shipping = 4.99;\n  }\n  else if (total > 0) {\n    shipping = 12.99;\n  }\n\n  const tax = (total / 10).toFixed(2);\n\n  const grandTotal = (total + shipping + Number(tax)).toFixed(2);\n\n  const formatNumber = num => {\n    const precision = num.toFixed(2);\n    return Number(precision);\n  }\n\n  return (\n    <div>\n      <h4>Order Summary</h4>\n      <p>Items Ordered: {cart.length}</p>\n      <p>Product Price: {formatNumber(total)}</p>\n      <p><small>Shipping Cost: {shipping}</small></p>\n      <p><small>Tax + VAT: {tax}</small></p>\n      <p>Total Price: {grandTotal}</p>\n      <br />\n      {\n        props.children\n      }\n    </div>\n  );\n};\n\nexport default Cart;","/home/bishnu/Desktop/Programming-Hero/projects/ema-john-simple/src/components/Review/Review.js",["107","108"],"import React, { useEffect, useState } from 'react';\nimport fakeData from '../../fakeData';\nimport { getDatabaseCart, processOrder, removeFromDatabaseCart } from '../../utilities/databaseManager';\nimport Cart from '../Cart/Cart';\nimport ReviewItem from '../ReviewItem/ReviewItem';\nimport happyImage from '../../images/giphy.gif';\nimport { useHistory } from 'react-router-dom';\n\nconst Review = () => {\n  const [cart, setCart] = useState([]);\n  const [orderPlaced, setOrderPlaced] = useState(false);\n\n  const history = useHistory();\n\n  const handleProceedCheckout = () => {\n    history.push('/shipment');\n  }\n\n  let thankYou;\n  if (orderPlaced) {\n    thankYou = <img src={happyImage} alt=\"\"/>;\n  }\n\n  const handleRemoveProduct = (productKey) => {\n    const newCart = cart.filter(pd => pd.key !== productKey);\n    setCart(newCart);\n    removeFromDatabaseCart(productKey);\n  }\n\n  useEffect(() => {\n    const savedCart = getDatabaseCart();\n    const productKeys = Object.keys(savedCart);\n    const cartProducts = productKeys.map(key => {\n      const product = fakeData.find(pd => pd.key === key);\n      product.quantity = savedCart[key];\n      return product;\n    })\n    setCart(cartProducts);\n  }, [])\n\n  return (\n    <div className='shop-container'>\n      <div className='product-container'>\n        {\n          cart.map(pd => <ReviewItem handleRemoveProduct={handleRemoveProduct} product={pd} key={pd.key}></ReviewItem>)\n        }\n        {\n          thankYou\n        }\n      </div>\n      <div className='cart-container'>\n        <Cart cart={cart}>\n          <button onClick={handleProceedCheckout} className='shop-btn'>Proceed Checkout</button>\n        </Cart>\n      </div>\n    </div>\n  );\n};\n\nexport default Review;","/home/bishnu/Desktop/Programming-Hero/projects/ema-john-simple/src/components/Inventory/Inventory.js",[],"/home/bishnu/Desktop/Programming-Hero/projects/ema-john-simple/src/components/NotFound/NotFound.js",[],"/home/bishnu/Desktop/Programming-Hero/projects/ema-john-simple/src/components/ProductDetail/ProductDetail.js",[],"/home/bishnu/Desktop/Programming-Hero/projects/ema-john-simple/src/utilities/databaseManager.js",[],"/home/bishnu/Desktop/Programming-Hero/projects/ema-john-simple/src/components/ReviewItem/ReviewItem.js",[],"/home/bishnu/Desktop/Programming-Hero/projects/ema-john-simple/src/components/Login/Login.js",["109"],"import { useContext, useState } from 'react';\nimport { UserContext } from \"../../App\";\nimport { useHistory, useLocation } from \"react-router-dom\";\nimport { createUserWithEmailAndPassword, handleFbSignIn, handleGoogleSignIn, handleSignOut, initializeLoginFramework, signInWithEmailAndPassword } from './loginManager';\n\n\nfunction Login() {\n  initializeLoginFramework();\n\n  const [user, setUser] = useState({\n    isSignedIn: false,\n    name: '',\n    email: '',\n    password: '',\n    photo: '',\n    error: '',\n    success: false\n  });\n  const [newUser, setNewUser] = useState(false);\n  const [loggedInUser, setLoggedInUser] = useContext(UserContext);\n\n  const history = useHistory();\n  const location = useLocation();\n  let { from } = location.state || { from: { pathname: '/' } };\n\n\n  // google sign in\n  const googleSignIn = () => {\n    handleGoogleSignIn()\n      .then(res => {\n        handleResponse(res, true);\n      })\n  }\n\n  // facebook sign in\n  const fbSignIn = () => {\n    handleFbSignIn()\n      .then(res => {\n        handleResponse(res, true);\n      })\n  }\n\n  // sign out\n  const signOut = () => {\n    handleSignOut()\n      .then(res => {\n        handleResponse(res, false);\n      })\n  }\n\n  const handleResponse = (res, redirect) => {\n    setUser(res);\n    setLoggedInUser(res);\n    if (redirect) {\n      history.replace(from);\n    }\n  }\n\n  const handleInputBlur = (e) => {\n    let isFieldValid = true;\n    // debugger;\n    if (e.target.name === 'email') {\n      isFieldValid = /\\S+@\\S+\\.\\S+/.test(e.target.value);\n    }\n    if (e.target.name === 'password') {\n      const isPasswordValid = e.target.value.length > 6;\n      const hasPasswordNumber = /\\d{1}/.test(e.target.value);\n      isFieldValid = isPasswordValid && hasPasswordNumber;\n    }\n\n    if (isFieldValid) {\n      const newUserInfo = { ...user };\n      newUserInfo[e.target.name] = e.target.value;\n      setUser(newUserInfo);\n    }\n  }\n\n  const handleSubmit = (e) => {\n    // create user with email and password\n    if (newUser && user.email && user.password) {\n      createUserWithEmailAndPassword(user.name, user.email, user.password)\n        .then(res => {\n          handleResponse(res, true);\n        })\n    }\n    // sign in with email & password\n    if (!newUser && user.email && user.password) {\n      signInWithEmailAndPassword(user.email, user.password)\n        .then(res => {\n          handleResponse(res, true);\n        })\n    }\n    e.preventDefault();\n  }\n\n  return (\n    <div style={{ textAlign: 'center' }}>\n      <h2>React Authentication Using Firebase</h2>\n      <br />\n      {\n        user.isSignedIn ? <button onClick={signOut}>Sign out</button>\n          : <button onClick={googleSignIn}>Google Sign in</button>\n      }\n      {\n        user.isSignedIn && <div>\n          <p>Welcome, {user.name}</p>\n          <p>Your Email: {user.email}</p>\n          <img src={user.photo} style={{ width: '25%' }} alt=\"\" />\n        </div>\n      }\n      <br /> <br />\n      <button onClick={fbSignIn}>Sign in using Facebook</button>\n\n      <h2>Our own Authentication</h2>\n      <input type=\"checkbox\" name=\"newUser\" onChange={() => setNewUser(!newUser)} id=\"\" />\n      <label htmlFor=\"newUser\">New User Sign up</label>\n\n      <form onSubmit={handleSubmit}>\n        {newUser && <input type=\"text\" name=\"name\" onBlur={handleInputBlur} placeholder=\"Your Name\" />}\n        <br />\n        <input type=\"text\" name=\"email\" onBlur={handleInputBlur} placeholder=\"Your Email Address\" required />\n        <br />\n        <input type=\"password\" name=\"password\" onBlur={handleInputBlur} id=\"\" placeholder=\"Your Password\" required />\n        <br />\n        <input type=\"submit\" value={newUser ? 'Sign up' : 'Sign in'} />\n      </form>\n      <p style={{ color: 'red' }}>{user.error}</p>\n      { user.success && <p style={{ color: 'green' }}>User {newUser ? 'created' : 'logged in'} successfully.</p>}\n    </div>\n  );\n}\n\nexport default Login;\n","/home/bishnu/Desktop/Programming-Hero/projects/ema-john-simple/src/components/Shipment/Shipment.js",["110"],"import React, { useContext } from 'react';\nimport { useForm } from 'react-hook-form';\nimport { UserContext } from '../../App';\nimport './Shipment.css';\n\nconst Shipment = () => {\n  const [loggedInUser, setLoggedInUser] = useContext(UserContext);\n  const { register, handleSubmit, watch, errors } = useForm();\n\n  const onSubmit = data => {\n    console.log('form submitted', data)\n  };\n\n  console.log(watch(\"example\")); // watch input value by passing the name of it\n\n  return (\n    <form onSubmit={handleSubmit(onSubmit)} className='ship-form'>\n      <input name=\"name\" defaultValue={loggedInUser.name} ref={register({ required: true })} placeholder='Your Name' />\n      {errors.name && <span className='error'>Name is required</span>}\n      \n      <input name=\"email\" defaultValue={loggedInUser.email} ref={register({ required: true })} placeholder='Your Email' />\n      {errors.email && <span className='error'>Email is required</span>}\n      \n      <input name=\"address\" ref={register({ required: true })} placeholder='Your Address' />\n      {errors.address && <span className='error'>Address is required</span>}\n      \n      <input name=\"phone\" ref={register({ required: true })} placeholder='Your Phone Number' />\n      {errors.phone && <span className='error'>Phone Number is required</span>}\n      \n      <input type=\"submit\" />\n    </form>\n  );\n};\n\nexport default Shipment;","/home/bishnu/Desktop/Programming-Hero/projects/ema-john-simple/src/components/Login/firebase.config.js",[],"/home/bishnu/Desktop/Programming-Hero/projects/ema-john-simple/src/components/PrivateRoute/PrivateRoute.js",["111"],"import React, { useContext } from 'react';\nimport { Redirect, Route } from 'react-router-dom';\nimport { UserContext } from '../../App';\n\nconst PrivateRoute = ({ children, ...rest }) => {\n  const [loggedInUser, setLoggedInUser] = useContext(UserContext);\n\n  return (\n    <Route\n      {...rest}\n      render={({ location }) =>\n        loggedInUser.email ? (\n          children\n        ) : (\n            <Redirect\n              to={{\n                pathname: \"/login\",\n                state: { from: location }\n              }}\n            />\n          )\n      }\n    />\n  );\n};\n\nexport default PrivateRoute;","/home/bishnu/Desktop/Programming-Hero/projects/ema-john-simple/src/components/Login/loginManager.js",["112","113","114"],"import 