rorCount":0,"fixableWarningCount":0,"usedDeprecatedRules":"44"},{"filePath":"84","messages":"85","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0,"usedDeprecatedRules":"44"},"/home/bishnu/Desktop/Programming-Hero/projects/ema-john-simple/src/index.js",[],["86","87"],"/home/bishnu/Desktop/Programming-Hero/projects/ema-john-simple/src/App.js",["88"],"/home/bishnu/Desktop/Programming-Hero/projects/ema-john-simple/src/reportWebVitals.js",[],"/home/bishnu/Desktop/Programming-Hero/projects/ema-john-simple/src/components/Header/Header.js",[],"/home/bishnu/Desktop/Programming-Hero/projects/ema-john-simple/src/components/Shop/Shop.js",["89"],"import React, { useEffect, useState } from 'react';\nimport { Link } from 'react-router-dom';\nimport fakeData from '../../fakeData';\nimport { addToDatabaseCart, getDatabaseCart } from '../../utilities/databaseManager';\nimport Cart from '../Cart/Cart';\nimport Product from '../Product/Product';\nimport './Shop.css';\n\nconst Shop = () => {\n  const first10 = fakeData.slice(0, 10);\n\n  const [products, setProducts] = useState(first10);\n  const [cart, setCart] = useState([]);\n\n  useEffect(() => {\n    const savedCart = getDatabaseCart();\n    const productKeys = Object.keys(savedCart);\n    const previousCart = productKeys.map(existingKey => {\n      const product = fakeData.find(pd => pd.key === existingKey);\n      product.quantity = savedCart[existingKey];\n      return product;\n    })\n    setCart(previousCart);\n  }, [])\n\n  const handleAddProduct = (product) => {\n    const sameProduct = cart.find(pd => pd.key === product.key);\n    let count = 1;\n    let newCart;\n    if (sameProduct) {\n      count = sameProduct.quantity + 1;\n      sameProduct.quantity = count;\n      const others = cart.filter(pd => pd.key !== sameProduct.key);\n      newCart = [...others, sameProduct];\n    }\n    else {\n      product.quantity = 1;\n      newCart = [...cart, product];\n    }\n    setCart(newCart);\n    addToDatabaseCart(product.key, count);\n  }\n\n  return (\n    <div className='shop-container'>\n      <div className='product-container'>\n        {\n          products.map(product => <Product showAddToCart={true} product={product} handleAddProduct={handleAddProduct} key={product.key}></Product>)\n        }\n      </div>\n      <div className='cart-container'>\n        <Cart cart={cart}>\n          <Link to='/review'>\n            <button className='shop-btn'>Review Order</button>\n          </Link>\n        </Cart>\n      </div>\n    </div>\n  );\n};\n\nexport default Shop;","/home/bishnu/Desktop/Programming-Hero/projects/ema-john-simple/src/fakeData/index.js",[],"/home/bishnu/Desktop/Programming-Hero/projects/ema-john-simple/src/fakeData/android.js",[],"/home/bishnu/Desktop/Programming-Hero/projects/ema-john-simple/src/fakeData/laptop.js",[],"/home/bishnu/Desktop/Programming-Hero/projects/ema-john-simple/src/fakeData/camera.js",[],"/home/bishnu/Desktop/Programming-Hero/projects/ema-john-simple/src/components/Product/Product.js",[],"/home/bishnu/Desktop/Programming-Hero/projects/ema-john-simple/src/components/Cart/Cart.js",["90"],"import React from 'react';\nimport { Link } from 'react-router-dom';\n\nconst Cart = (props) => {\n  const cart = props.cart;\n\n  // const total = cart.reduce((total, product) => total + product.price, 0);\n  let total = 0;\n  for (let i = 0; i < cart.length; i++) {\n    const product = cart[i];\n    total = total + product.price * product.quantity;\n  }\n\n  let shipping = 0;\n  if (total > 35) {\n    shipping = 0;\n  }\n  else if (total > 15) {\n    shipping = 4.99;\n  }\n  else if (total > 0) {\n    shipping = 12.99;\n  }\n\n  const tax = (total / 10).toFixed(2);\n\n  const grandTotal = (total + shipping + Number(tax)).toFixed(2);\n\n  const formatNumber = num => {\n    const precision = num.toFixed(2);\n    return Number(precision);\n  }\n\n  return (\n    <div>\n      <h4>Order Summary</h4>\n      <p>Items Ordered: {cart.length}</p>\n      <p>Product Price: {formatNumber(total)}</p>\n      <p><small>Shipping Cost: {shipping}</small></p>\n      <p><small>Tax + VAT: {tax}</small></p>\n      <p>Total Price: {grandTotal}</p>\n      <br />\n      {\n        props.children\n      }\n    </div>\n  );\n};\n\nexport default Cart;","/home/bishnu/Desktop/Programming-Hero/projects/ema-john-simple/src/components/Review/Review.js",["91","92"],"import React, { useEffect, useState } from 'react';\nimport fakeData from '../../fakeData';\nimport { getDatabaseCart, processOrder, removeFromDatabaseCart } from '../../utilities/databaseManager';\nimport Cart from '../Cart/Cart';\nimport ReviewItem from '../ReviewItem/ReviewItem';\nimport happyImage from '../../images/giphy.gif';\nimport { useHistory } from 'react-router-dom';\n\nconst Review = () => {\n  const [cart, setCart] = useState([]);\n  const [orderPlaced, setOrderPlaced] = useState(false);\n\n  const history = useHistory();\n\n  const handleProceedCheckout = () => {\n    history.push('/shipment');\n  }\n\n  let thankYou;\n  if (orderPlaced) {\n    thankYou = <img src={happyImage} alt=\"\"/>;\n  }\n\n  const handleRemoveProduct = (productKey) => {\n    const newCart = cart.filter(pd => pd.key !== productKey);\n    setCart(newCart);\n    removeFromDatabaseCart(productKey);\n  }\n\n  useEffect(() => {\n    const savedCart = getDatabaseCart();\n    const productKeys = Object.keys(savedCart);\n    const cartProducts = productKeys.map(key => {\n      const product = fakeData.find(pd => pd.key === key);\n      product.quantity = savedCart[key];\n      return product;\n    })\n    setCart(cartProducts);\n  }, [])\n\n  return (\n    <div className='shop-container'>\n      <div className='product-container'>\n        {\n          cart.map(pd => <ReviewItem handleRemoveProduct={handleRemoveProduct} product={pd} key={pd.key}></ReviewItem>)\n        }\n        {\n          thankYou\n        }\n      </div>\n      <div className='cart-container'>\n        <Cart cart={cart}>\n          <button onClick={handleProceedCheckout} className='shop-btn'>Proceed Checkout</button>\n        </Cart>\n      </div>\n    </div>\n  );\n};\n\nexport default Review;","/home/bishnu/Desktop/Programming-Hero/projects/ema-john-simple/src/components/Inventory/Inventory.js",[],"/home/bishnu/Desktop/Programming-Hero/projects/ema-john-simple/src/components/NotFound/NotFound.js",[],"/home/bishnu/Desktop/Programming-Hero/projects/ema-john-simple/src/components/ProductDetail/ProductDetail.js",[],"/home/bishnu/Desktop/Programming-Hero/projects/ema-john-simple/src/utilities/databaseManager.js",[],"/home/bishnu/Desktop/Programming-Hero/projects/ema-john-simple/src/components/ReviewItem/ReviewItem.js",[],"/home/bishnu/Desktop/Programming-Hero/projects/ema-john-simple/src/components/Login/Login.js",["93","94","95","96","97"],"/home/bishnu/Desktop/Programming-Hero/projects/ema-john-simple/src/components/Shipment/Shipment.js",[],"/home/bishnu/Desktop/Programming-Hero/projects/ema-john-simple/src/components/Login/firebase.config.js",[],{"ruleId":"98","replacedBy":"99"},{"ruleId":"100","replacedBy":"101"},{"ruleId":"102","severity":1,"message":"103","line":8,"column":3,"nodeType":"104","messageId":"105","endLine":8,"endColumn":7},{"ruleId":"102","severity":1,"message":"106","line":12,"column":20,"nodeType":"104","messageId":"105","endLine":12,"endColumn":31},{"ruleId":"102","severity":1,"message":"103","line":2,"column":10,"nodeType":"104","messageId":"105","endLine":2,"endColumn":14},{"ruleId":"102","severity":1,"message":"107","line":3,"column":27,"nodeType":"104","messageId":"105","endLine":3,"endColumn":39},{"ruleId":"102","severity":1,"message":"108","line":11,"column":23,"nodeType":"104","messageId":"105","endLine":11,"endColumn":37},{"ruleId":"102","severity":1,"message":"109","line":23,"column":10,"nodeType":"104","messageId":"105","endLine":23,"endColumn":22},{"ruleId":"102","severity":1,"message":"110","line":58,"column":13,"nodeType":"104","messageId":"105","e